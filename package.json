{
  "name": "url-js",
  "version": "0.2.0",
  "description": "Simple URL parser, similar to DOM URL",
  "main": "url.js",
  "file": [
    "url.js"
  ],
  "scripts": {
    "build": "node build",
    "test": "mocha --require should --reporter spec",
    "verup": "verup"
  },
  "verup": {
    "files": [
      "bower.json",
      "url.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/duzun/URL.js.git"
  },
  "keywords": [
    "URL",
    "DOM",
    "Parse",
    "Parser",
    "URI",
    "http",
    "hostname",
    "pathname"
  ],
  "author": "Dumitru Uzun <contanct@duzun.me> (https://DUzun.Me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/duzun/URL.js/issues"
  },
  "homepage": "https://github.com/duzun/URL.js#readme",
  "devDependencies": {
    "mocha": "*",
    "should": "*",
    "verup": "^1.x"
  },
  "jshintConfig": {
    "laxbreak": true,
    "laxcomma": true,
    "undef": true,
    "unused": true,
    "sub": true,
    "expr": true,
    "bitwise": false,
    "eqeqeq": false,
    "boss": true,
    "eqnull": true,
    "scripturl": true,
    "-W041": false
  }
}
